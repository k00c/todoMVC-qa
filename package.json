{
  "name": "todomvc-qa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint . --ext .ts,.js --plugin playwright",
    "lint:fix": "npx eslint . --ext .ts,.js --plugin playwright --fix",
    "check": "npm run format:check && npm run lint",
    "fix": "npm run format && npm run lint:fix",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "report": "npx playwright show-report",
    "analyze": "node analyze-test-performance.js",
    "test:analyze": "npm test && npm run analyze",
    "postinstall": "npm update browserstack-node-sdk",
    "lint-browserstack": "npx eslint . --ext .ts,.js --plugin browserstack-node-sdk playwright",
    "lint:fix-browserstack": "npx eslint . --ext .ts,.js --plugin browserstack-node-sdk playwright --fix",
    "test-browserstack": "npx browserstack-node-sdk playwright test",
    "test:headed-browserstack": "npx browserstack-node-sdk playwright test --headed",
    "test:debug-browserstack": "npx browserstack-node-sdk playwright test --debug",
    "report-browserstack": "npx browserstack-node-sdk playwright show-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "browserstack-node-sdk": "^1.45.3",
    "eslint-plugin-playwright": "^2.2.2"
  }
}
